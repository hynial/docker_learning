[mysqld]
skip-host-cache
skip-name-resolve
max_allowed_packet=32M
collation-server=utf8mb4_general_ci
init-connect='SET NAMES utf8mb4'
character-set-server=utf8mb4
wait_timeout=300
interactive_timeout=300
default-time-zone = "+08:00"

[client]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4
